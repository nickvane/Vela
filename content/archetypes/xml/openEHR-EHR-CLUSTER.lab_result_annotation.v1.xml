  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Laboratory result annotation</items>
        <items id="description">To record comments and other per-node annotations within a lab_test archetype.</items>
      </items>
      <items code="at0001">
        <items id="text">Unexpected result indicator</items>
        <items id="description">Indicates the degree of diagnostic significance associated with a pathology test result based on all the available clinical information.</items>
      </items>
      <items code="at0002">
        <items id="text">Result comment</items>
        <items id="description">A comment on an individual lab result.</items>
      </items>
      <items code="at0003">
        <items id="text">Testing method</items>
        <items id="description">Description of the method use for an individual result</items>
      </items>
      <items code="at0004">
        <items id="text">Result status</items>
        <items id="description">The status of an individual lab result.</items>
      </items>
      <items code="at0005">
        <items id="text">Reference range guidance</items>
        <items id="description">Further advice on the applicability of the reference range.</items>
      </items>
      <items code="at0006">
        <items id="text">Specimen detail</items>
        <items id="description">Details of  the specimen for a particular reb result.</items>
      </items>
      <items code="at0007">
        <items id="text">Result name</items>
        <items id="description">Name of the result to which this annotation refers. This will often be coded with a reference terminology such as LOINC.</items>
      </items>
      <items code="at0008">
        <items id="text">Interim</items>
        <items id="description">This is an initial or interim result - further updates are anticipated.</items>
      </items>
      <items code="at0009">
        <items id="text">Final</items>
        <items id="description">This is the final result. No further alterations are anticipated</items>
      </items>
      <items code="at0010">
        <items id="text">Supplementary</items>
        <items id="description">This is a supplememtary result in addition to the Interim result.</items>
      </items>
      <items code="at0011">
        <items id="text">Corrected (amended)</items>
        <items id="description">This is a Correction which should replace any previous results.</items>
      </items>
      <items code="at0012">
        <items id="text">Aborted</items>
        <items id="description">The testing/ reporting process was aborted and never completed.</items>
      </items>
      <items code="at0013">
        <items id="text">Never performed</items>
        <items id="description">The test was never performed.</items>
      </items>
      <items code="at0014">
        <items id="text">Laboratory details</items>
        <items id="description">Demographic details of the laboratory with responsibility for the individual result. Details of secondary laboratories may also be included.</items>
      </items>
      <items code="at0015">
        <items id="text">Laboratory result identifier</items>
        <items id="description">The identifier given to the laboratory test result of a pathology 
investigation.</items>
      </items>
      <items code="at0017">
        <items id="text">Link to original lab test</items>
        <items id="description">If the result was originally reported in  a separate lab test entry, link to that entry.</items>
      </items>
    </term_definitions>
  </ontology>
