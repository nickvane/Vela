  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Level of exertion</items>
        <items id="description">Record information about level of exertion.</items>
      </items>
      <items code="at0005">
        <items id="text">Measured</items>
        <items id="description">The measured level of exertion.</items>
      </items>
      <items code="at0006">
        <items id="text">At rest</items>
        <items id="description">The person is at rest, prior to undertaking exercise.</items>
      </items>
      <items code="at0007">
        <items id="text">During exertion</items>
        <items id="description">The person is exerting themselves at the time.</items>
      </items>
      <items code="at0008">
        <items id="text">Post-exertion</items>
        <items id="description">Measurement is taken after exertion has ceased.</items>
      </items>
      <items code="at0009">
        <items id="text">Phase</items>
        <items id="description">The phase or context of exercise.</items>
      </items>
      <items code="at0010">
        <items id="text">Exercise intensity</items>
        <items id="description">Amount of work being done during exercise.</items>
      </items>
      <items code="at0011">
        <items id="text">Intensity</items>
        <items id="description">Semiquantitative description of the intensity of exercise undertaken.</items>
      </items>
      <items code="at0012">
        <items id="text">Low Intensity</items>
        <items id="description">Up to 80% Maximal Heart Rate.</items>
      </items>
      <items code="at0013">
        <items id="text">Medium Intensity</items>
        <items id="description">80-85% of Maximal Heart Rate.</items>
      </items>
      <items code="at0014">
        <items id="text">High Intensity</items>
        <items id="description">85-90% Maximal Heart Rate.</items>
      </items>
      <items code="at0015">
        <items id="text">Flat Out</items>
        <items id="description">90-100% Maximal Heart Rate.</items>
      </items>
      <items code="at0016">
        <items id="text">Description</items>
        <items id="description">Description of the exertion.</items>
      </items>
    </term_definitions>
