      <items code="at0000">
        <items id="text">Intravascular pressure</items>
        <items id="description">The pressure in a specific location, blood vessel or heart cavity, at a specific phase of the heart or an average over the heart cycle.</items>
      </items>
      <items code="at0001">
        <items id="text">history</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
