  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Change</items>
        <items id="description">Record details about readiness to change current status.</items>
      </items>
      <items code="at0001">
        <items id="text">Readiness to change</items>
        <items id="description">Record person's inclination to change current status.</items>
      </items>
      <items code="at0002">
        <items id="text">Reasons for current use of substance</items>
        <items id="description">Record person's reasons for current use of substance.</items>
      </items>
      <items code="at0003">
        <items id="text">Triggers for use</items>
        <items id="description">Identify triggers for person to use substance.</items>
      </items>
      <items code="at0004">
        <items id="text">Perceptions about current use</items>
        <items id="description">Person's perceptions about their use of the substance.</items>
      </items>
      <items code="at0005">
        <items id="text">Positive</items>
        <items id="description">Record why the person likes to use substance.</items>
      </items>
      <items code="at0006">
        <items id="text">Negative</items>
        <items id="description">Record why the person doesn't like to use substance.</items>
      </items>
      <items code="at0007">
        <items id="text">Perceptions about ceasing current use</items>
        <items id="description">Person's perceptions about stopping use of substance.</items>
      </items>
      <items code="at0008">
        <items id="text">Positive</items>
        <items id="description">Record why the person would like to cease their use of substance.</items>
      </items>
      <items code="at0009">
        <items id="text">Negative</items>
        <items id="description">Record why the person would not like to cease their use of substance.</items>
      </items>
      <items code="at0010">
        <items id="text">Barriers to ceasing use</items>
        <items id="description">Identify barriers which prevent person from ceasing use.</items>
      </items>
      <items code="at0011">
        <items id="text">Comment</items>
        <items id="description">Comment about person's preparedness for changing their status.</items>
      </items>
      <items code="at0012">
        <items id="text">Not ready (Precontemplation)</items>
        <items id="description">These people are not seriously considering changing in the next 6 months.</items>
      </items>
      <items code="at0013">
        <items id="text">Unsure (Contemplation)</items>
        <items id="description">These perople are seriously considering changing in the next 6 months.</items>
      </items>
      <items code="at0014">
        <items id="text">Ready (Preparation)</items>
        <items id="description">These people are planning to change in the next 30 days.</items>
      </items>
      <items code="at0015">
        <items id="text">Action</items>
        <items id="description">Includes former users who have changed in the last 6 months.</items>
      </items>
      <items code="at0016">
        <items id="text">Maintenance</items>
        <items id="description">These are users who changed over 6 months ago.</items>
      </items>
    </term_definitions>
  </ontology>
