  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Tumour- Lymph node metastases</items>
        <items id="description">To record findings of tumour metastases in lymph nodes.</items>
      </items>
      <items code="at0001">
        <items id="text">Number of nodes examined</items>
        <items id="description">Number of nodes examined.</items>
      </items>
      <items code="at0002">
        <items id="text">Number of nodes with tumour</items>
        <items id="description">Number of nodes which show tumour involvement.</items>
      </items>
      <items code="at0009">
        <items id="text">Extent of tumour</items>
        <items id="description">Extent of tumour expressed as a maximum length. Applies only to an individual node.</items>
      </items>
      <items code="at0010">
        <items id="text">Lymph node site location</items>
        <items id="description">The anatomical location of the lymph node or site being reported.</items>
      </items>
      <items code="at0011">
        <items id="text">Nature of involvement</items>
        <items id="description">Nature or grade of the tumour, if present.</items>
      </items>
      <items code="at0012">
        <items id="text">Focal</items>
        <items id="description">The tumour is of a focal nature.</items>
      </items>
      <items code="at0013">
        <items id="text">Diffuse</items>
        <items id="description">The tumour is of a diffuse nature.</items>
      </items>
      <items code="at0014">
        <items id="text">Extent of extranodal tumour</items>
        <items id="description">Extent of extranodal tumour expressed as a maximum length.</items>
      </items>
      <items code="at0024">
        <items id="text">Complete</items>
        <items id="description">The node is completely invaded by tumour.</items>
      </items>
      <items code="at0025">
        <items id="text">Tumour involvement</items>
        <items id="description">Findings of lymph node involvement with tumour.</items>
      </items>
      <items code="at0026">
        <items id="text">Present - left side nodes only</items>
        <items id="description">Tumour is present in left sided nodes only.</items>
      </items>
      <items code="at0027">
        <items id="text">Present - right side nodes only</items>
        <items id="description">Tumour is present in right side nodes only.</items>
      </items>
      <items code="at0028">
        <items id="text">Present - bilateral nodes</items>
        <items id="description">Tumour is present in nodes on both sides.</items>
      </items>
      <items code="at0029">
        <items id="text">Absent</items>
        <items id="description">Tumour is absent from the lymph node site.</items>
      </items>
      <items code="at0030">
        <items id="text">Equivocal</items>
        <items id="description">Lymph node involvement by tumour is equivocal.</items>
      </items>
      <items code="at0031">
        <items id="text">Indeterminate</items>
        <items id="description">Llymph node involvement by tumour has not been determined.</items>
      </items>
      <items code="at0033">
        <items id="text">Indeterminate</items>
        <items id="description">The nature of tumour has not been determnined.</items>
      </items>
      <items code="at0034">
        <items id="text">Extra-capsular extension</items>
        <items id="description">Findings of extension of tumour beyond a node capsule.</items>
      </items>
      <items code="at0035">
        <items id="text">Present</items>
        <items id="description">Extra-capsular involvement by tumour is present.</items>
      </items>
      <items code="at0036">
        <items id="text">Absent</items>
        <items id="description">Extra-capsular involvement by tumour is absent.</items>
      </items>
      <items code="at0037">
        <items id="text">Equivocal</items>
        <items id="description">Extra-capsular involvement by tumour is equivocal.</items>
      </items>
      <items code="at0038">
        <items id="text">Indeterminate</items>
        <items id="description">Extra-capsular involvement by tumour has not been determined.</items>
      </items>
      <items code="at0039">
        <items id="text">Extra-nodal extension</items>
        <items id="description">Findings related to extension of tumour external to the nodal capsule.</items>
      </items>
      <items code="at0040">
        <items id="text">Description</items>
        <items id="description">A text description of lymph node involvement by tumour.</items>
      </items>
      <items code="at0041">
        <items id="text">Lymph node site name</items>
        <items id="description">The name for the lymph node site being reported. May be a general location e.g. 'Axillary nodes', a relative site e.g. 'Sentinel nodes' or a individual node.</items>
      </items>
      <items code="at0042">
        <items id="text">Present</items>
        <items id="description">Lymph node involvement with tumour is present.</items>
      </items>
      <items code="at0044">
        <items id="text">Tissue available</items>
        <items id="description">Has the appropriate lymph node tissue been made available for examination?</items>
      </items>
      <items code="at0045">
        <items id="text">Lymph node details</items>
        <items id="description">Further details of lymph node metastasis at this site e.g individual nodes or further levels</items>
      </items>
      <items code="at0046">
        <items id="text">Route of involvement</items>
        <items id="description">The route by which the tumour became involved in lymph node tissue.</items>
      </items>
      <items code="at0047">
        <items id="text">Direct spread</items>
        <items id="description">The tumour involved the lymph node by direct spread.</items>
      </items>
      <items code="at0048">
        <items id="text">Metastasis</items>
        <items id="description">The tumour involved the lymph node by metastasis.</items>
      </items>
      <items code="at0049">
        <items id="text">Present</items>
        <items id="description">Lymph node tissue is present.</items>
      </items>
      <items code="at0050">
        <items id="text">Absent</items>
        <items id="description">Lymph node tissue is absent.</items>
      </items>
      <items code="at0051">
        <items id="text">Marker dye uptake</items>
        <items id="description">Findings of whether marker dye has been taken up by the lymph node or lymph node group.</items>
      </items>
      <items code="at0052">
        <items id="text">Radioactivity count</items>
        <items id="description">Radioactivity count measured after use of radiocolloid.</items>
      </items>
    </term_definitions>
  </ontology>
</archetype>

