    <term_definitions language="en">
      <items code="at0.3">
        <items id="text">Global Statement</items>
        <items id="description">Global statements about the exclusion. This can be used to capture any information that is needed to be explicitly recorded as being absent or excluded within the record.</items>
      </items>
      <items code="at0.4">
        <items id="text">No significant family history</items>
        <items id="description">No significant family history of any problem or diagnosis.</items>
      </items>
      <items code="at0.5">
        <items id="text">No relevant family history</items>
        <items id="description">No relevant family history of any problem or diagnosis.</items>
      </items>
      <items code="at0.6">
        <items id="text">No significant family history of</items>
        <items id="description">Positive statement about significant problems and diagnoses in related family members that are explicitly known to have not been identified at the time of recording.</items>
      </items>
      <items code="at0.7">
        <items id="text">No relevant family history of</items>
        <items id="description">Positive statement about relevant problems and diagnoses in related family members that are explicitly known to have not been identified at the time of recording.</items>
      </items>
      <items code="at0000">
        <items id="text">Exclusion statement</items>
        <items id="description">Statements that need to be positively recorded as absent or excluded.</items>
      </items>
      <items code="at0000.1">
        <items id="text">Exclusion statement - Family History</items>
        <items id="description">Statements about Family History that need to be positively recorded as absent or excluded.</items>
      </items>
